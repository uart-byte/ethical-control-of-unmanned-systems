			<xsd:element ref="SignalOfInterest"/>
			<xsd:element ref="Contaminant"/>
			<xsd:element ref="GoalStatus"/>
			<xsd:element ref="VehicleStatus"/>
		</xsd:choice>
		<xsd:attributeGroup ref="CommonAttributes"/>
	</xsd:complexType>
	<xsd:complexType name="uuvScriptCommandsType">
		<xsd:annotation>
			<xsd:documentation>Extends generic script commands to add those applicable to unmanned underwater vehicles.</xsd:documentation>
		</xsd:annotation>
		<xsd:choice maxOccurs="unbounded">
